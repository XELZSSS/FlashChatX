{
  "name": "flashchat-x",
  "private": true,
  "version": "v1.7.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel proxy client",
    "client": "vite",
    "build": "vite build",
    "analyze": "set ANALYZE=true&& vite build",
    "preview": "vite preview",
    "proxy": "node server/proxy.js",
    "electron:start": "electron .",
    "electron:wait": "wait-on http://localhost:3000 && npm run electron:start",
    "electron:dev": "npm-run-all --parallel dev electron:wait",
    "electron:build:win": "npm run build && electron-builder --config electron-builder.config.cjs --win",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,html,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,html,md}\"",
    "lint": "eslint . --max-warnings 100",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@google/genai": "^1.34.0",
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.13.2",
    "emoji-mart": "^5.6.0",
    "lucide-react": "^0.562.0",
    "mammoth": "^1.10.0",
    "memu-js": "^0.2.1",
    "openai": "^6.15.0",
    "pdfjs-dist": "^4.10.38",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "tesseract.js": "^5.1.1",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@resvg/resvg-js-cli": "^2.6.2-beta.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^5.1.2",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "npm-run-all": "^4.1.5",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "wait-on": "^9.0.3"
  },
  "build": {
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    }
  }
}
